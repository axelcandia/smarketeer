    // BEGIN LOGIN
    .content
      // BEGIN LOGIN FORM
      form.login-form(action="/login" method="post")
        h3.form-title Wellcome!
        .alert.alert-danger.display-hide
          button.close(data-close='alert')
          span
            | Enter your email and password.
        .form-group
          // ie8, ie9 does not support html5 placeholder, so we just show field title for that
          label.control-label.visible-ie8.visible-ie9 Email
          input.form-control.form-control-solid.placeholder-no-fix(type='text', autocomplete='off', placeholder='Email',name="email")
        .form-group
          label.control-label.visible-ie8.visible-ie9 Password
          input.form-control.form-control-solid.placeholder-no-fix(type='password', autocomplete='off', placeholder='Password', name='password')
        .login-options
          h4 Sign in using
          ul.social-icons
            li
              a.social-icon-color.facebook(data-original-title='facebook', href='/auth/facebook')
            li
              a.social-icon-color.twitter(data-original-title='Twitter', href='/auth/twitter')

            li
              a.social-icon-color.googleplus(data-original-title='Goole Plus', href='javascript:;')
            //li
              a.social-icon-color.linkedin(data-original-title='Linkedin', href='javascript:;')
        .login-options
          h4  Not an user? 
            a.form-title( href = "/register" ) Click here to register
        .create-account 
          p
            button#register-btn(type="submit") Sign In
